<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml"
          fx:controller="assignment2.ClientGuiController" fx:id="mainbody">
    <padding>
        <Insets bottom="25" left="25" right="25" top="25"></Insets>
    </padding>

    <stylesheets>
        <URL value="@clientform.css"></URL>
    </stylesheets>

    <Text GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" fx:id="header"></Text>

    <HBox GridPane.columnIndex="1" GridPane.rowIndex="3" alignment="TOP_LEFT" spacing="10">
        <Button onAction="#handleSubmitButtonAction" text="Alternative 1"></Button>
    </HBox>
</GridPane>
