<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ERROR">
            <AppenderRef ref="Console"/>
        </Root>


        <Logger name="no.wact.jenjon13.Client" level="trace">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="no.wact.jenjon13.Server" level="trace">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>
